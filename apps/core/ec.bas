BYTE A = 128
BYTE C = 0
BYTE E = 0
@START
	LET C = #COM[A]
	IF C = 0 GOTO DONE
	GOSUB PUTC
	INC A
	GOTO START
DONE:
	LET C = 10
	GOSUB PUTC
	GETPID A
	LET #COM[1] = A
	LET #COM[0] = 255
HALT:
	FORFIT
	GOTO HALT
PUTC:
	WRITE #SIO,C,E
	IF E > 0 GOTO PERR
	RETURN
PERR:
	FORFIT
	GOTO PUTC